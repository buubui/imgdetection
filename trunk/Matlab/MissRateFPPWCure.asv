function val = MissRateFPPWCure(M, numpos, thr)
n = size(M,1);
nTh = size(thr,2);
val = zeros(nTh,2);

for i=1:nTh
    fp = 0;
    tp = 0;
    fn = 0;
    %calc fp
    for j=numpos+1:n
        if M(j,1)>= thr(1,i)
            fp = fp + 1;
        end
    end
    % calc tp
    for j=1:numpos
        if M(j,1) >= thr(1,i)
            tp = tp + 1;
        end
    end
    fn = numpos - tp;
    val(i,1) = fp/n;
    val(i,2) = fn/(fp+tp);
end
plot(val(:,1),val(:,2));
end